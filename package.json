{
  "name": "workshop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite --port 5174",
    "deploy": "vite build && gh-pages -d dist",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:5174 cy:open-e2e"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "cypress": "^13.6.6",
    "framer-motion": "^11.0.6",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "start-server-and-test": "^2.0.3",
    "sweetalert2": "^11.10.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.1.4"
  }
}
